let alturaEnCm = 180;
let alturaEnM = alturaEnCm / 100;
let pesoEnKg = 75.5;
let alturaEnMArriba = Math.ceil(alturaEnM);
let pesoEnKgAbajo = Math.floor(pesoEnKg);
let maxValueCheck = (Number.MAX_VALUE + 1) === Number.MAX_VALUE;


